; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="TowerVerse"
config/description="The cross platform app of TowerVerse!"
run/main_scene="res://assets/Scenes/StartMenu.tscn"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.156863, 0.156863, 0.156863, 1 )
config/icon="res://icon.png"
config/quit_on_go_back=false

[autoload]

Uuid="*res://assets/Scripts/UUID.gd"
Save="*res://assets/Scripts/Save.gd"
Socket="*res://assets/Scripts/Socket.gd"
Utils="*res://assets/Scripts/Utils.gd"
Variables="*res://assets/Scripts/Variables.gd"
Templates="*res://assets/Scripts/Templates.gd"

[debug]

settings/crash_handler/message="An error has occured, TowerVerse will exit."

[display]

window/size/width=1920
window/size/height=1080
window/size/resizable=false
window/size/test_width=1920
window/size/test_height=1080
window/dpi/allow_hidpi=true
window/vsync/use_vsync=false
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"
window/stretch/aspect="expand"

